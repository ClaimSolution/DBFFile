{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "removeComments": false,
        "sourceMap": true,
        "declaration": true,
        "outDir": "../built",
        "lib": ["es6"],

        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
    }
}
